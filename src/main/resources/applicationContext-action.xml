<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-4.0.xsd
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">

	<!-- 好像貌似必须使用 scope prototype -->
	
	<bean id="indexAction" name="indexAction" class="com.star.action.IndexAction" scope="prototype">
	</bean>
	
	<bean id="mapAction" name="mapAction" class="com.star.action.MapAction" scope="prototype">
	</bean>
	
	<bean id="userAction" name="userAction" class="com.star.action.UserAction" scope="prototype">
	</bean>
	
	<bean id="demoAction" name="demoAction" class="com.star.action.DemoAction" scope="prototype">
	</bean>

</beans>
